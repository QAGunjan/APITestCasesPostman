							"listen": "test",
							"script": {
								"exec": [
									"var responseJson =pm.response.json();\r",
									"\r",
									"pm.environment.set(\"booking_id\", responseJson.bookingid);\r",
									"pm.environment.set(\"firstname\", responseJson.booking.firstname);\r",
									"pm.environment.set(\"lastname\", responseJson.booking.lastname);\r",
									"pm.environment.set(\"totalprice\", responseJson.booking.totalprice);\r",
									"\r",
									"pm.test(\"Validate additionalneeds value is Breakfast\", function()\r",
									"{\r",
									"  pm.expect(responseJson.booking.additionalneeds).to.eql(\"Breakfast\"); \r",
									"});\r",
									"\r",
									"  pm.test(\"Validate Status code is 200\", function () {\r",
									"        pm.response.to.have.status(200);\r",
									"  });\r",
									"pm.test(\"Validate Content-Type is present\", function () {\r",
									"    pm.response.to.have.header(\"Content-Type\");\r",
									"});\r",
									"pm.test(\"Validate Response time is less than 1500ms\", function () {\r",
									"    pm.expect(pm.response.responseTime).to.be.below(1500);\r",
									"});\r",
									"pm.test(\"Status code name has string\", function () {\r",
									"    pm.response.to.have.status(\"OK\");\r",
									"});\r",
									"\r",
